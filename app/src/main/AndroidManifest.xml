<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liqy.mvp">

    <application
        android:name=".MvpApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".home.HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login.LoginActivity"
            android:label="MVP with Login Showcase"></activity>
        <activity
            android:name=".loginoptimized.LoginOptimizedActivity"
            android:label="Optimized MVP with Login Showcase"></activity>
        <activity
            android:name=".outteradapter.AdapterActivityA"
            android:label="MVP in Adapter A"></activity>
        <activity
            android:name=".outteradapter.AdapterActivityB"
            android:label="MVP in Adapter B"></activity>
    </application>

</manifest>
